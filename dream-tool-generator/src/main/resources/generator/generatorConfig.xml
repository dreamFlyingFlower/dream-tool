<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="demo" defaultModelType="flat" targetRuntime="MyBatis3">

		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<property name="schema" value="simpleoa" />
		<property name="suppressDate" value="true" />
		<property name="suppressAllComments" value="true" />
		<property name="addRemarkComments" value="true" />
		<property name="generateModelExample" value="true" />
		<property name="generateMapper" value="false" />
		<property name="generateMapperXml" value="true" />

		<plugin type="com.simple.plugins.MapperPlugin"/>

		<!-- 通用实体类,Mapper,xml生成 -->
		<plugin type="com.simple.plugins.CommonPlugin" />

		<!-- 连接数据库配置 -->
		<jdbcConnection driverClass="com.mysql.cj.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/simpleoa?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=false&amp;serverTimezone=Asia/Shanghai"
			userId="root" password="52LDforever">
			<property name="nullCatalogMeansCurrent" value="true" />
		</jdbcConnection>

		<javaModelGenerator targetProject="src\main\java\" targetPackage="com.wy.model">
			<property name="constructorBased" value="false" />
			<property name="enableSubPackages" value="false" />
			<property name="immutable" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- 生成Mapper对应的xml文件地址 -->
		<sqlMapGenerator targetProject="src\main\resources\" targetPackage="mappers" />
		
		<!-- 生成的Mapper文件地址 -->
		<javaClientGenerator type="XMLMAPPER" targetProject="src\main\java\" targetPackage="com.wy.mapper" />

		<table tableName="ts_menu" domainObjectName="Menu">
			<generatedKey column="menu_id" sqlStatement="JDBC" />
		</table>
		<table tableName="tb_operate_log" domainObjectName="OperateLog">
			<generatedKey column="operate_id" sqlStatement="JDBC" />
		</table>
		<table tableName="tb_login_log" domainObjectName="LoginLog">
			<generatedKey column="login_id" sqlStatement="JDBC" />
		</table>
		<table tableName="ts_user" domainObjectName="User">
			<generatedKey column="user_id" sqlStatement="JDBC" />
		</table>
		<table tableName="tb_apply" domainObjectName="Apply">
			<generatedKey column="apply_id" sqlStatement="JDBC" />
		</table>
		<table tableName="tr_role_depart" domainObjectName="RoleDepart">
		</table>
		<table tableName="ts_depart" domainObjectName="Depart">
			<generatedKey column="depart_id" sqlStatement="JDBC" />
		</table>
		<table tableName="ts_salary" domainObjectName="Salary">
			<generatedKey column="salary_id" sqlStatement="JDBC" />
		</table>
		<table tableName="tr_role_menu" domainObjectName="RoleMenu">
		</table>
		<table tableName="tr_user_role" domainObjectName="UserRole">
		</table>
		<table tableName="tr_role_button" domainObjectName="RoleButton">
		</table>
		<table tableName="ts_notice" domainObjectName="Notice">
			<generatedKey column="notice_id" sqlStatement="JDBC" />
		</table>
		<table tableName="ts_role" domainObjectName="Role">
			<generatedKey column="role_id" sqlStatement="JDBC" />
		</table>
		<table tableName="tb_sign" domainObjectName="Sign">
			<generatedKey column="sign_id" sqlStatement="JDBC" />
		</table>
		<table tableName="ts_fileinfo" domainObjectName="Fileinfo">
			<generatedKey column="file_id" sqlStatement="JDBC" />
		</table>
		<table tableName="ts_button" domainObjectName="Button">
			<generatedKey column="button_id" sqlStatement="JDBC" />
		</table>
		<table tableName="ts_dict" domainObjectName="Dict">
			<generatedKey column="dict_id" sqlStatement="JDBC" />
		</table>
		<table tableName="ts_userinfo" domainObjectName="Userinfo">
		</table>
		<!-- 自动从数据库中生成uuid赋值给VARCHAR类型主键 -->
		<!-- <table tableName="ti_user" domainObjectName="User"> -->
		<!-- <generatedKey column="user_id" -->
		<!-- sqlStatement="select replace(uuid(),'-','') from dual" identity="false" /> -->
		<!-- </table> -->
	</context>
</generatorConfiguration>